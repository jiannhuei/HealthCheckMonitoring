{
  "ConnectionStrings": {
    "SqlConnectionString": "SomeConnectionString"
  },
  "Mode": "Dev",
  "HealthCheckEndpoint": {
    "DB2Endpoint": "https://apihub.publicbankgroup.com/Check/Health"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Log\\log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "Serilog.WebApplication"
    }
  }
}